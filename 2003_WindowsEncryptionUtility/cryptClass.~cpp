#include <iostream>
#include "cryptClass.h"

void cryptIt::encryptIt(char* tempString){

	for(register unsigned int i(0);i<(strlen(tempString));++i){
		tempString[i] = (~tempString[i]);
		tempString[i]^=(i);
	}
}

void cryptIt::decryptIt(char* tempString){

	for(register unsigned int i(0);i<(strlen(tempString));++i){
		tempString[i] = (~tempString[i]);
		tempString[i]^=(i);
	}
}
